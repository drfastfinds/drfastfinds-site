[
  {% for page in site.pages %}
    {% unless page.url == "/search.json" %}
      {
        "title": "{{ page.title | escape }}",
        "url": "https://drfastfinds.github.io/drfastfinds-site{{ page.url }}",
        "content": "{{ page.content | strip_html | escape }}"
      }{% unless forloop.last %},{% endunless %}
    {% endunless %}
  {% endfor %}
]
