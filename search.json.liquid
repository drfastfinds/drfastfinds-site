[
  {% assign pages = site.pages | where_exp: "page", "page.url != '/search.json'" %}
  {% for page in pages %}
    {
      "title": "{{ page.title | escape | json_escape }}",
      "url": "https://drfastfinds.github.io/drfastfinds-site{{ page.url }}",
      "content": "{{ page.content | strip_html | escape | json_escape }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
